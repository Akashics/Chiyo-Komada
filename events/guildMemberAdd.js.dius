exports.run = async (client, member) => {

    //console.log(member.user)
    //console.log('Avatar URL = ' + member.user.avatarURL() || member.user.defaultAvatarURL)
    if (client.settings.guilds.get(member.guild.id).logChannel) {
        const embed = new client.methods.Embed()
            .setColor(200 * 256)
            .setAuthor(`${member.user.username} #${member.user.discriminator} [${member.user.id}]`, member.user.avatarURL())
            .setTimestamp()
            .setFooter('Joined', client.user.avatarURL());
        return client.channels.get(client.settings.guilds.get(member.guild.id).logChannel).sendEmbed(embed);
    }
}